graft GPT_Cov_Let

include LICENSE
include README.md

include setup.py
include bumpversion.cfg
include .bumpversion.cfg
include pyproject.toml
include Makefile

prune .github
exclude .gitignore
exclude .gitattributes

# Patterns to exclude from any directory
global-exclude *~
global-exclude *.swp
global-exclude *.pyc
global-exclude *.pyo
global-exclude .git
global-exclude .ipynb_checkpoints
global-exclude .DS_Store

include *.md
recursive-include GPT_Cov_Let *.html
recursive-include GPT_Cov_Let *.js
recursive-include GPT_Cov_Let *.swp
recursive-include build *.html
recursive-include build *.js
recursive-include build *.py
recursive-include build *.swp

recursive-include docs *.bat
recursive-include docs *.py
recursive-include docs *.rst
recursive-include docs Makefile

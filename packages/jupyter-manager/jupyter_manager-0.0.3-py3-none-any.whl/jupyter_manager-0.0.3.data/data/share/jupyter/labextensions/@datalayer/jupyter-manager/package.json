{
  "name": "@datalayer/jupyter-manager",
  "version": "0.0.2",
  "description": "UI and CLI to manage your Jupyter stack.",
  "keywords": [
    "jupyter",
    "jupyterhub",
    "jupyterlab",
    "jupyterlab-extension",
    "manager"
  ],
  "homepage": "https://github.com/datalayer/jupyter-manager",
  "bugs": {
    "url": "https://github.com/datalayer/jupyter-manager/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Datalayer",
    "email": "info@datalayer.io"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/datalayer/jupyter-manager.git"
  },
  "scripts": {
    "build": "jlpm build:lib && jlpm build:labextension:dev",
    "build:prod": "jlpm clean && jlpm build:lib && jlpm build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "clean": "jlpm clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:lintcache": "rimraf .eslintcache .stylelintcache",
    "clean:labextension": "rimraf datalayer/labextension",
    "clean:all": "jlpm clean:lib && jlpm clean:labextension && jlpm clean:lintcache",
    "eslint": "jlpm eslint:check --fix",
    "eslint:check": "eslint . --cache --ext .ts,.tsx",
    "install:extension": "jlpm build",
    "lint": "jlpm stylelint && jlpm prettier && jlpm eslint",
    "lint:check": "jlpm stylelint:check && jlpm prettier:check && jlpm eslint:check",
    "prettier": "jlpm prettier:base --write --list-different",
    "prettier:base": "prettier \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
    "prettier:check": "jlpm prettier:base --check",
    "stylelint": "jlpm stylelint:check --fix",
    "stylelint:check": "stylelint --cache \"style/**/*.css\"",
    "test": "jest --coverage",
    "start": "run-p -c start:*",
    "start:webpack": "webpack serve",
    "start:jupyter-server": "cd ./dev/sh && ./start-jupyter-server.sh",
    "jupyterhub": "run-p -c jupyterhub:*",
    "jupyterhub:start": "jupyterhub -f ./dev/config/jupyterhub_config.py",
    "jupyterhub:watch": "yarn start",
    "jupyterlab": "run-p -c jupyterlab:*",
    "jupyterlab:start": "jupyter lab --config=./dev/config/jupyter_server_config.py",
    "jupyterlab:watch": "run-p -c watch:src watch:labextension",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w",
    "watch:labextension": "jupyter labextension watch .",
    "kill": "make kill || true"
  },
  "dependencies": {
    "@datalayer/icons": "0.0.10",
    "@datalayer/rjsf-primer": "0.0.7",
    "@jupyterlab/application": "4.0.0-beta.1",
    "@jupyterlab/coreutils": "6.0.0-beta.1",
    "@jupyterlab/services": "7.0.0-beta.1",
    "@jupyterlab/settingregistry": "4.0.0-beta.1",
    "@primer/octicons-react": "18.2.0",
    "@primer/react": "35.22.0",
    "@rjsf/core": "5.3.0",
    "@rjsf/utils": "5.3.0",
    "@rjsf/validator-ajv6": "5.3.0",
    "bootstrap": "^4.5.3",
    "lodash.debounce": "4.0.8",
    "react": "18.2.0",
    "react-bootstrap": "2.7.4",
    "react-dom": "18.2.0",
    "react-icons": "4.8.0",
    "react-multi-select-component": "^3.0.7",
    "react-redux": "7.2.4",
    "react-router-dom": "6.6.0",
    "recompose": "npm:react-recompose@^0.31.2",
    "redux": "4.0.5",
    "regenerator-runtime": "^0.13.9",
    "styled-components": "4.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.0",
    "@jupyterlab/builder": "4.0.0-beta.1",
    "@jupyterlab/launcher": "4.0.0-beta.1",
    "@jupyterlab/testutils": "4.0.0-beta.1",
    "@types/jest": "29.4.0",
    "@types/node": "18.11.9",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "4.8.1",
    "@typescript-eslint/parser": "4.8.1",
    "babel-loader": "9.1.2",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "html-webpack-plugin": "5.3.1",
    "jest": "29.4.3",
    "jest-playwright-preset": "3.0.1",
    "mkdirp": "1.0.3",
    "npm-run-all": "4.1.5",
    "playwright": "1.31.1",
    "prettier": "2.1.1",
    "prop-types": "15.8.1",
    "rimraf": "3.0.2",
    "style-loader": "2.0.0",
    "stylelint": "14.3.0",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-recommended": "6.0.0",
    "stylelint-config-standard": "~24.0.0",
    "stylelint-prettier": "2.0.0",
    "ts-jest": "29.0.5",
    "typescript": "5.0.3",
    "webpack": "5.74.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.3"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "jupyter_manager"
        }
      }
    },
    "extension": true,
    "outputDir": "jupyter_manager/labextension",
    "schemaDir": "schema",
    "_build": {
      "load": "static/remoteEntry.44916ced8ea8187e9f24.js",
      "extension": "./extension",
      "style": "./style"
    }
  },
  "jupyter-releaser": {
    "hooks": {
      "before-build-npm": [
        "python -m pip install jupyterlab==4.0.0b1",
        "jlpm"
      ],
      "before-build-python": [
        "jlpm clean:all"
      ]
    }
  }
}
